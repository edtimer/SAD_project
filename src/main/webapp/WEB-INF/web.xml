<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">

	<display-name>SDA_teamc_project</display-name>
	<servlet>
		<servlet-name>submit</servlet-name>
		<servlet-class>com.filter.ProcessUpload</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>submit</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Converter</servlet-name>
		<servlet-class>com.pipe.ConverterController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Converter</servlet-name>
		<url-pattern>/ConverterController</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	  	<welcome-file>SAgile_demo.jsp</welcome-file>
	</welcome-file-list>


</web-app>
